{
	"name": "react-template",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"test": "craco test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"airbnb-base",
			"prettier"
		],
		"rules": {
			"import/no-unresolved": 0,
			"import/extensions": 0
		}
	},
	"lint-staged": {
		"**/*.js": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@craco/craco": "^6.4.3",
		"@emotion/css": "^11.7.1",
		"@emotion/react": "^11.7.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"autoprefixer": "^9.8.8",
		"axios": "^0.24.0",
		"http-proxy-middleware": "^2.0.1",
		"postcss": "^7.0.39",
		"qs": "^6.10.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.1",
		"react-scripts": "4.0.3",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
		"web-vitals": "^1.0.1"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.16.7",
		"@emotion/babel-plugin": "^11.7.2",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.2.3",
		"prettier": "^2.4.1",
		"prettier-eslint": "^13.0.0"
	}
}
